{
    "name": "easeappphp/highper",
    "description": "High-performance PHP microservice framework with extreme concurrency support",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "amphp/http-client": "^5.0",
        "amphp/http-server": "^3.0",
        "amphp/http-server-router": "^2.0",
        "amphp/http-server-static-content": "^2.0",
        "amphp/log": "^2.0",
        "amphp/mysql": "^3.0",
        "amphp/parallel": "^2.3",
        "amphp/socket": "^2.0",
        "amphp/websocket-server": "^2.0",
        "filp/whoops": "^2.15",
        "illuminate/container": "^10.0",
        "nyholm/psr7": "^1.8",
        "open-telemetry/exporter-otlp": "^1.0",
        "open-telemetry/sdk": "^1.0",
        "psr/container": "^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0 || ^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/log": "^1.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "^6.0",
        "phpbench/phpbench": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "EaseAppPHP\\HighPer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EaseAppPHP\\HighPer\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "EaseAppPHP Team",
            "email": "team@easeapphighper.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.0"
        }
    }
}